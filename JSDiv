<!DOCTYPE html>
<html>
  <head>
    <script>
     

    function pageLoaded(){




      let div = document.getElementById(" new element added ")
      div.innerText = "Created by JS"
      
      }
      </script>
  </head>
  <body onload="pageLoaded();">
    

     <p
     id = "div">loaded</p>
     
    
  </body>
</html>
